{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Enrich": [ "FromLogContext", "WithThreadId", "WithMachineName", "WithEnvironmentName" ],
    "Properties": {
      "Application": "SquidlrApi"
    }
  },
  "ApplicationInsights": {
    "EnablePerformanceCounterCollectionModule": false
  },
  "AllowedHosts": "*",
  "Squidlr": {
    "FacebookHostUri": "https://www.facebook.com",
    "InstagramHostUri": "https://www.instagram.com",
    "LinkedInHostUri": "https://www.linkedin.com",
    "TiktokHostUri": "https://www.tiktok.com",
    "TwitterAuthorizationBearerToken": "AAAAAAAAAAAAAAAAAAAAANRILgAAAAAAnNwIzUejRCOuH5E6I8xnZz4puTs%3D1Zv7ttfk8LF81IUq16cHjhLTvJu4FA33AGWWjCpTnA",
    "TwitterApiHostUri": "https://api.twitter.com",
    "ProxyOptions": {
      "UseProxy": true
    }
  }
}
