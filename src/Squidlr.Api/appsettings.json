{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System.Net.Http.HttpClient.TwitterWebClient.LogicalHandler": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.ApplicationInsights" ],
    "Enrich": [ "FromLogContext", "WithThreadId", "WithMachineName", "WithEnvironmentName" ],
    "WriteTo": [
      {
        "Name": "ApplicationInsights",
        "Args": {
          "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
        }
      }
    ],
    "Properties": {
      "Application": "TweetCrawlerApi"
    }
  },
  "ApplicationInsights": {
    "EnablePerformanceCounterCollectionModule": false
  },
  "AllowedHosts": "*",
  "TweetCrawler": {
    "AuthorizationBearerToken": "AAAAAAAAAAAAAAAAAAAAANRILgAAAAAAnNwIzUejRCOuH5E6I8xnZz4puTs%3D1Zv7ttfk8LF81IUq16cHjhLTvJu4FA33AGWWjCpTnA",
    "TwitterApiHostUri": "https://api.twitter.com"
  }
}
